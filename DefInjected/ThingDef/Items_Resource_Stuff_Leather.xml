  <Leather_Rhinoceros.stuffProps.stuffAdjective>แผ่นหนังแรด</Leather_Rhinoceros.stuffProps.stuffAdjective>
  
  <!-- EN: thrumbofur -->
  <Leather_Thrumbo.label>แผ่นหนังธรัมโบ</Leather_Thrumbo.label>
